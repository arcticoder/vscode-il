{
    "name": "vscode-ac-il",
    "displayName": "acIL",
    "description": "IL Language support",
    "version": "0.0.2",
    "publisher": "arcticoder",
    "private": true,
    "author": {
        "name": "arcticoder",
        "email": "arcticoder@duck.com",
        "url": "https://github.com/arcticoder"
    },
    "bugs": {
        "url": "https://github.com/arcticoder/vscode-il/issues",
        "email": "arcticoder@duck.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arcticoder/vscode-il.git"
    },
    "homepage": "https://github.com/arcticoder/vscode-il/blob/master/README.md",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "il",
                "aliases": [
                    "IL",
                    "il",                    
                    "CIL",
                    "MSIL"
                ],
                "extensions": [
                    ".il"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "il",
                "scopeName": "source.il",
                "path": "./syntaxes/il.json"
            }
        ]
    }
}
