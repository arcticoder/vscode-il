{
    "name": "vscode-il",
    "displayName": "IL",
    "description": "IL Language support",
    "version": "0.0.1",
    "publisher": "soltys",
    "private": true,
    "author": {
        "name": "Paweł Sołtysiak",
        "email": "pawel+illanguage@soltysiak.it",
        "url": "https://github.com/soltys/vscode-il"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "il",
                "aliases": [
                    "IL",
                    "il",                    
                    "CIL",
                    "MSIL"
                ],
                "extensions": [
                    ".il"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "il",
                "scopeName": "source.il",
                "path": "./syntaxes/il.json"
            }
        ]
    }
}